# Copy this file to .env and fill in your values.
# .env is git-ignored – never commit real tokens.

# Infrahub server address (default: localhost)
INFRAHUB_ADDRESS=http://localhost:8000

# Infrahub API token – create one via:
#   Admin UI → Settings → API Tokens → + New
#   or via GraphQL mutation CoreAccountTokenCreate
INFRAHUB_API_TOKEN=your-token-here
